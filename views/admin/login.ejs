<% include ../layout/header %>
<div class="container">
	<div class="row">
		<div class="col-md-5">
			<form action="/login/to" method="post">
				<div class="form-group">
					<label for="exampleInputEmail1">Email address</label>
					<input type="text" name="user" class="form-control" id="exampleInputEmail1" placeholder="Email">
				</div>
				<div class="form-group">
					<label for="exampleInputPassword1">Password</label>
					<input type="password" name="passwd" class="form-control" id="exampleInputPassword1" placeholder="Password">
				</div>
				<div class="checkbox">
					<label>
						<input type="checkbox"> Check me out
					</label>
				</div>
				<button type="submit" class="btn btn-default">登录</button>
				<a href="/users/reg">点击注册</a>
			</form>
		</div>
	</div>
</div>
<% include ../layout/footer %>
